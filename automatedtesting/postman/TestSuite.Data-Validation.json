 {
 	"info": {
 		"_postman_id": "fa0388ae-2f53-4ea8-a0c9-eef940bd3640",
 		"name": "Starter APIs Data Validation",
 		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
 	},
 	"item": [
 		{
 			"name": "Validate Employee Data",
 			"event": [
 				{
 					"listen": "test",
 					"script": {
 						"exec": [
 							"pm.test(\"Response is not empty\", function () {",
 							"    pm.response.to.be.ok;",
 							"});",
                             							"",
 							"pm.test(\"Response contains valid user data\", function () {",
 							"    var jsonData = pm.response.json();",
 							"    pm.expect(jsonData).to.be.an('array');",
 							"    pm.expect(jsonData[0]).to.have.property('id');",
 							"    pm.expect(jsonData[0]).to.have.property('email');",
 							"    pm.expect(jsonData[0]).to.have.property('firstName');",
 							"    pm.expect(jsonData[0]).to.have.property('lastName');",
							"    pm.expect(jsonData[0]).to.have.property('avatar');",
 							"});",
 							"",
 							"pm.test(\"Status code is 200\", function () {",
 							"    pm.response.to.have.status(200);",
 							"});"
 						],
 						"type": "text/javascript"
 					}
 				}
 			],
 			"request": {
 				"method": "GET",
 				"header": [],
 				"url": {
 					"raw": "https://reqres.in/api/users?page=2",
 					"protocol": "https",
					 "host": [
						"reqres",
						"in"
					 ],
					 "path": [
						 "api",
						 "users?page2"
					 ]
 				}
 			},
 			"response": []

 		}
 	]
 }
